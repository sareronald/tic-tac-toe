           }}\n                hasActivity={activities.some(\n                  (activity) => activity.currentSquare === \"2\"\n                )}\n              />\n              <Square\n                onClick={() => {\n                  selectActivity(3);\n                  openModal();\n                }}\n                hasActivity={activities.some(\n                  (activity) => activity.currentSquare === \"3\"\n                )}\n              />\n            </div>\n            <div className=\"board-row\">\n              <Square\n                onClick={() => {\n                  selectActivity(4);\n                  openModal();\n                }}\n                hasActivity={activities.some(\n                  (activity) => activity.currentSquare === \"4\"\n                )}\n              />\n              <Square\n                onClick={() => {\n                  selectActivity(5);\n                  openModal();\n                }}\n                hasActivity={activities.some(\n                  (activity) => activity.currentSquare === \"5\"\n                )}\n              />\n              <Square\n                onClick={() => {\n                  selectActivity(6);\n                  openModal();\n                }}\n                hasActivity={activities.some(\n                  (activity) => activity.currentSquare === \"6\"\n                )}\n              />\n            </div>\n            <div className=\"board-row\">\n              <Square\n                onClick={() => {\n                  selectActivity(7);\n                  openModal();\n                }}\n                hasActivity={activities.some(\n                  (activity) => activity.currentSquare === \"7\"\n                )}\n              />\n              <Square\n                onClick={() => {\n                  selectActivity(8);\n                  openModal();\n                }}\n                hasActivity={activities.some(\n                  (activity) => activity.currentSquare === \"8\"\n                )}\n              />\n              <Square\n                onClick={() => {\n                  selectActivity(9);\n                  openModal();\n                }}\n                hasActivity={activities.some(\n                  (activity) => activity.currentSquare === \"9\"\n                )}\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default TictactoeBoard;\n","/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/components/Instructions/Instructions.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/components/Modal/Modal.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/components/Login/Login.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/components/Signup/Signup.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/pages/Dashboard/Dashboard.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/components/Modal/ModalActivityForm.js",["95"],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/components/Square/Square.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/utils/actions.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/components/AboutTictactoe/AboutTictactoe.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/components/NewTictactoe/NewTictactoe.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/store/appStore.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/store/index.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/utils/userFunctions.js",[],"/Users/sarahronald/usyd-syd-activities/21-Project-3/tic-tac-toe/client/src/compo